        </activity>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="af8f17a0afa49c3175c6567ca4f79e35a9bc9ace"
        />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>